{
  "name": "server",
  "version": "1.0.0",
  "description": "REST API to mock a basic wallet system.",
  "main": "index.js",
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "scripts": {
    "start-client": "ts-node src/index.ts",
    "start": "npm run start-prod-client",
    "build": "tsc",
    "clean": "rm -rf ./dist/*",
    "start-prod-client": "node ./dist/index.js",
    "db-create": "node ./dist/db.js create",
    "db-migrate": "npm run build && node ./dist/db.js migrate",
    "test": "mocha -r ts-node/register --recursive ./src/test/test.ts"
  },
  "author": "dkafewou",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.18.2",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-watch": "^7.7.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "command-line-commands": "^3.0.2",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "joi": "^17.6.0",
    "knex": "^2.1.0",
    "moment": "^2.29.3",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.7.3",
    "pg-promise": "^10.11.1",
    "pino": "^8.0.0",
    "pino-pretty": "^8.0.0",
    "semver": "^7.3.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/command-line-commands": "^2.0.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.40",
    "@types/pino": "^7.0.5",
    "@types/semver": "^7.3.9",
    "@types/sinon": "^10.0.11",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "eslint": "^8.17.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.16",
    "sinon": "^14.0.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "engines": {
    "node": "14.x.x",
    "npm": ">=6"
  }
}
